-- допустим есть пара объектов неизвестного типа ?
local a :: ? = A(3, "three");
local b :: ? = fun(B(c, i)) { c.string };
local c :: ? = 35;


{
  -- пусть мы хотим в рантайме проверять использование объектом типа подобного вида
  obj :: Union[A(Int, Str), B(C, Int) ~> Str];
  local obj;

  -- оба присвоения должны проверять в рантайме, что мы присваиваем объект правильного типа
  obj := a; -- OK
  obj := b; -- OK
  obj := c; -- Runtime error
  Unit
};
Unit