-- checking matching of expressions
fun asdf(x) {
  x :: A(Int);
  case x of
      A (0) -> "1"
    | A (x) -> "2"
    -- | A (x, y) -> "3" -- Error: Branch does not match anything
  esac
}

fun len(x) {
  -- @x(t) :: Union [Nil, Cons(t, x)]; -- хочется задавать рекурсивные типы примерно так, но пока что их нет
  x :: Union [Nil, Cons(?, ?)];
  case x of
     Nil -> 0
   | Cons (_, xs) -> len(xs)
